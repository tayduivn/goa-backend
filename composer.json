{
  "name": "slim/tsl-transport",
  "description": "Api Rest to the company TSL",
  "keywords": [
    "microframework",
    "rest",
    "router",
    "psr7"
  ],
  "homepage": "http://github.com/slimphp/Slim-Skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Del Pino",
      "email": "ivans1041@gmail.com",
      "homepage": "http://www.theroom.com.ve"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "slim/slim": "^3.1",
    "slim/php-view": "^2.0",
    "monolog/monolog": "^1.17",
    "robmorgan/phinx": "^0.10.6",
    "fzaninotto/faker": "^1.8",
    "dyorg/slim-token-authentication": "^0.3.3",
    "tuupola/slim-jwt-auth": "^3.3",
    "bryanjhv/slim-session": "~3.0",
    "akrabat/rka-slim-session-middleware": "^2.0",
    "wisembly/elephant.io": "^3.3",
    "workerman/phpsocket.io": "^1.1",
    "cboden/ratchet": "^0.4.1"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8 < 6.0"
  },
  "autoload": {
    "psr-4": {
      "Tests\\": "tests/",
      "App\\Lib\\": "libraries/",
      "App\\Controller\\": "controllers/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public public/index.php",
    "test": "phpunit"
  }
}
